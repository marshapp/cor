set terminal pngcairo size 1000, 600 font "Helvetica, 18"

set style line 1 lc rgb "red" lw 2 lt 1 pt 7 ps 1
set style line 2 lc rgb "blue" lw 2 lt 1 pt 7 ps 1
set style line 3 lc rgb "green" lw 2 lt 1 pt 7 ps 1

set xlabel "x"
set ylabel "T ()"

set output "analitica.png"

set xrange[-0.001:0.021]
set yrange[35:55]

plot "resultats_analitica.txt" u 1:2 w l ls 1 t "10", "resultats_analitica.txt" u 1:3 w l ls 2 t "100", "resultats_analitica.txt" u 1:4 w l ls 3 t "1000"

set output